<extension xmlns="http://ns.adobe.com/air/extension/21.0">
	<id>com.vsdevelop.air.extension.toast</id>
	<versionNumber>1.0</versionNumber>
	<platforms>
		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>ANEToast.jar</nativeLibrary>
				<initializer>com.vsdevelop.air.extension.toast.ANEToast</initializer>
				<finalizer>com.vsdevelop.air.extension.toast.ANEToast</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-x86">
			<applicationDeployment>
				<nativeLibrary>ANEToast.jar</nativeLibrary>
				<initializer>com.vsdevelop.air.extension.toast.ANEToast</initializer>
				<finalizer>com.vsdevelop.air.extension.toast.ANEToast</finalizer>
			</applicationDeployment>
		</platform>

		<platform name="default">
			<applicationDeployment />
		</platform>
	</platforms>
</extension>
