<extension xmlns="http://ns.adobe.com/air/extension/21.0">
  <id>com.vsdevelop.air.extension.wechat</id>
  <versionNumber>1.0</versionNumber>
  <platforms>
    <platform name="Android-ARM">
      <applicationDeployment>
        <nativeLibrary>ANEWeChat.jar</nativeLibrary>
        <initializer>com.vsdevelop.air.extension.wechat.ANEWeChat</initializer>
        <finalizer>com.vsdevelop.air.extension.wechat.ANEWeChat</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="Android-x86">
      <applicationDeployment>
        <nativeLibrary>ANEWeChat.jar</nativeLibrary>
        <initializer>com.vsdevelop.air.extension.wechat.ANEWeChat</initializer>
        <finalizer>com.vsdevelop.air.extension.wechat.ANEWeChat</finalizer>
      </applicationDeployment>
    </platform>
    <platform name="default">
      <applicationDeployment/>
    </platform>
  </platforms>
</extension>
