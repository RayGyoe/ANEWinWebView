
<extension xmlns="http://ns.adobe.com/air/extension/21.0">
	<id>com.vsdevelop.air.extension.gyroscope</id>
	<versionNumber>1.0</versionNumber>
	<platforms>
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libgyroscopeExtension.a</nativeLibrary>
				<initializer>ADBEExtInitializer</initializer>
				<finalizer>ADBEExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>GyroscopeNative.jar</nativeLibrary>
				<initializer>com.vsdevelop.air.extension.gyroscope.GyroscopeExtension</initializer>
				<finalizer>com.vsdevelop.air.extension.gyroscope.GyroscopeExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-x86">
			<applicationDeployment>
				<nativeLibrary>GyroscopeNative.jar</nativeLibrary>
				<initializer>com.vsdevelop.air.extension.gyroscope.GyroscopeExtension</initializer>
				<finalizer>com.vsdevelop.air.extension.gyroscope.GyroscopeExtension</finalizer>
			</applicationDeployment>
		</platform>

		<platform name="default">
			<applicationDeployment />
		</platform>
	</platforms>
</extension>
